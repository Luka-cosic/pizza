                <!-- small box -->
                <div class="small-box bg-warning">
                  <div class="inner">
                    <h3><%= dostavljaci[i].name+" "+dostavljaci[i].lastName %></h3>
    
                    <p>44</p>
                  </div>
                  <div class="icon">
                    <i class="ion ion-person-add"></i>
                  </div>
                  <a href="#" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
                </div>
              </div>


        <% } %>
    </div>
        
        <!-- /.row (main row) -->
      </div><!-- /.container-fluid -->
    </section>
